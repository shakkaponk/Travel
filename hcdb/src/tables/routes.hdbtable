COLUMN TABLE "ROUTES" ( 
   "ROUTEID" INTEGER GENERATED BY DEFAULT AS IDENTITY, 
   "AIRLINECODE" NVARCHAR(2),
   "AIRLINENAME" NVARCHAR(256),
   "ALLIANCENAME" NVARCHAR(256),
   "AIRPORTCODEORIGIN" NVARCHAR(3) NOT NULL,
   "AIRPORTCODEDESTINATION" NVARCHAR(3) NOT NULL,   
   "DISTANCE" INTEGER, 
   "DURATION" INTEGER, 
   PRIMARY KEY ("ROUTEID") )
   